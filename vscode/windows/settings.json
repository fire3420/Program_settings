    "vim.easymotion": true,
    "vim.easymotionMarkerFontSize": 15,
    "vim.easymotionMarkerBackgroundColor": "blue",
    "vim.useSystemClipboard": true,
    "vim.leader": "\\",
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "leader",
                "o"
            ],
            "after": [
                "<C-m>",
                "o",
                "<Esc>"
            ]
        },
        {
            "before": [
                "leader",
                "O"
