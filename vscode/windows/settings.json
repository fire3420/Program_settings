{
    "python.formatting.provider": "black",
    "editor.rulers": [
        80,
        120
    ],
    "editor.mouseWheelZoom": true,
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "j"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.easymotion": true,
    "vim.easymotionMarkerFontSize": 15,
    "vim.easymotionMarkerBackgroundColor": "blue",
    "vim.useSystemClipboard": true,
    "vim.leader": "\\",
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "leader",
                "o"
            ],
            "after": [
                "<C-m>",
                "o",
                "<Esc>"
            ]
        },
        {
            "before": [
                "leader",
                "O"
            ],
            "after": [
                "<C-m>",
                "O",
                "<Esc>"
            ]
        },
        {
            "before": [
                "<C-w>"
            ],
            "commands": [
                ":tabclose"
            ]
        },
        {
            "before": [
                "<C-n>"
            ],
            "commands": [
                ":tabnew"
            ]
        },
        {
            "before": [
                "leader",
                "r"
            ],
            "after": [
                "<C-r>"
            ]
        },
        {
            "before": [
                "leader",
                "d"
            ],
            "after": [
                "\"",
                "_",
                "d"
            ]
        },
        {
            "before": [
                "leader",
                "1"
            ],
            "after": [
                "^"
            ]
        },
        {
            "before": [
                "leader",
                "2"
            ],
            "after": [
                "$"
            ]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "leader",
                "1"
            ],
            "after": [
                "^"
            ]
        },
        {
            "before": [
                "leader",
                "2"
            ],
            "after": [
                "g",
                "_"
            ]
        },
        {
            "before": [
                "leader",
                "p"
            ],
            "after": [
                "\"",
                "_",
                "d",
                "P"
            ]
        }
    ],
    "todo-tree.highlights.enabled": true,
    "terminal.integrated.inheritEnv": false,
    "window.zoomLevel": 0,
    "todo-tree.tree.showScanModeButton": false,
    "vim.hlsearch": true,
    "workbench.editor.enablePreview": false,
    "remote.SSH.remotePlatform": {
        "h6_tf1": "linux",
        "h6_tf3": "linux",
        "hm_pred": "linux",
        "hm_cron": "linux",
        "h6_tfpredict": "linux",
        "AMD_jyRL1": "linux",
        "AMD_jyRL2": "linux",
        "h6_rl2": "linux",
        "hm_tfpredict": "linux",
        "AMD_jyRL2_win": "linux",
        "AMD_jyRL3": "linux",
        "HP_jyRL3": "linux"
    },
    "editor.fontSize": 18,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "python.jediEnabled": false,
    "python.languageServer": "Microsoft",
    "editor.fontFamily": "'Droid Sans Mono', 'monospace', monospace",
    "vsnotes.defaultNotePath": "C:\\Users\\fire3\\OneDrive\\blog_secret\\_posts",
    // "vsnotes.defaultNotePath": "/home/john/Jungyong/Projects/gitlab_blog_mysecret/_posts",
    "vsnotes.defaultNoteTitle": "{dt}-memo_{dd}_{title}.{ext}",
    "vsnotes.templates": [
        "0quicknote",
        "1coin",
        "2stock",
        "3dev",
        "4paper",
        "5envops",
        "6unclassified",
        "7automation",
        "9diary",
    ],
    "vsnotes.tokens": [
        {
            "type": "datetime",
            "token": "{dt}",
            "format": "YYYY-MM-DD",
            "description": "Insert formatted datetime."
        },
        {
            "type": "datetime",
            "token": "{dd}",
            "format": "HH-mm",
            "description": "Insert formatted datetime."
        },
        {
            "type": "title",
            "token": "{title}",
            "description": "Insert note title from input box.",
            "format": "Untitled"
        },
        {
            "type": "extension",
            "token": "{ext}",
            "description": "Insert file vsnotes.",
            "format": "md"
        }
    ],
    "vsnotes.defaultNoteName": "Note",
    "workbench.colorTheme": "Monokai",
    "update.enableWindowsBackgroundUpdates": false,
    "files.autoGuessEncoding": true,
    "python.sortImports.args": [
        "-sp isort.cfg"
    ],
    "[python]": {
        "editor.codeActionsOnSave": {
            "source.organizeImports": true
        }
    },
    "vscode-mysql.resultTheme": "Dark"
}
