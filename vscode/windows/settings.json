        },
        {
            "before": [
                "<C-n>"
            ],
            "commands": [
                ":tabnew"
            ]
        },
        {
            "before": [
                "leader",
                "r"
            ],
            "after": [
                "<C-r>"
            ]
        },
        {
            "before": [
                "leader",
                "d"
            ],
            "after": [
                "\"",
                "_",
                "d"
            ]
        },
        {
            "before": [
                "leader",
                "1"
            ],
            "after": [
                "^"
            ]
        },
        {
            "before": [
                "leader",
                "2"
            ],
            "after": [
                "$"
            ]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "leader",
                "1"
            ],
            "after": [
                "^"
            ]
        },
        {
            "before": [
                "leader",
                "2"
            ],
            "after": [
                "g",
                "_"
            ]
        },
        {
            "before": [
                "leader",
                "p"
            ],
            "after": [
                "\"",
                "_",
                "d",
                "P"
            ]
        }
    ],
