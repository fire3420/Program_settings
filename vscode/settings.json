{
    "python.formatting.provider": "black",
    "editor.rulers": [
        80,
        120
    ],
    "editor.mouseWheelZoom": true,
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "j"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.easymotion": true,
    "vim.easymotionMarkerFontSize": 12,
    "vim.easymotionMarkerBackgroundColor": "red",
    "vim.useSystemClipboard": true,
    "vim.leader": "\\",
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "leader",
                "o"
            ],
            "after": [
                "<C-m>",
                "o",
                "<Esc>"
            ]
        },
        {
            "before": [
                "leader",
                "O"
            ],
            "after": [
                "<C-m>",
                "O",
                "<Esc>"
            ]
        },
        {
            "before": [
                "<C-w>"
            ],
            "commands": [
                ":tabclose"
            ]
        },
        {
            "before": [
                "<C-n>"
            ],
            "commands": [
                ":tabnew"
            ]
        },
        {
            "before": [
                "leader",
                "r"
            ],
            "after": [
                "<C-r>"
            ]
        },
        {
            "before": [
                "leader",
                "d"
            ],
            "after": [
                "\"",
                "_",
                "d"
            ]
        },
        {
            "before": [
                "leader",
                "1"
            ],
            "after": [
                "^"
            ]
        },
        {
            "before": [
                "leader",
                "2"
            ],
            "after": [
                "$"
            ]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "leader",
                "1"
            ],
            "after": [
                "^"
            ]
        },
        {
            "before": [
                "leader",
                "2"
            ],
            "after": [
                "g",
                "_"
            ]
        },
        {
            "before": [
                "leader",
                "p"
            ],
            "after": [
                "\"",
                "_",
                "d",
                "P"
            ]
        }
    ],
    "todo-tree.highlights.enabled": true,
    "workbench.editor.enablePreview": false,
    "terminal.integrated.inheritEnv": true,
    "window.zoomLevel": 0,
    "todo-tree.tree.showScanModeButton": false,
    "vim.hlsearch": true,
    "python.pythonPath": "/home/john/anaconda3/envs/pygdocs_linux/bin/python",
    "explorer.confirmDragAndDrop": false,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "python.jediEnabled": false,
    "python.languageServer": "Microsoft",
    "editor.fontFamily": "'Droid Sans Mono', 'monospace', monospace",
    "vsnotes.defaultNotePath": "/home/john/Jungyong/Projects/gitlab_blog_mysecret/_posts",
    "vsnotes.defaultNoteTitle": "{dt}-memo_{dd}_{title}.{ext}",
    "vsnotes.templates": [
        "0quicknote",
        "1coin",
        "2stock",
        "3dev",
        "4paper",
        "5envops",
        "6unclassified",
        "7automation",
        "9diary",
    ],
    "vsnotes.tokens": [
        {
            "type": "datetime",
            "token": "{dt}",
            "format": "YYYY-MM-DD",
            "description": "Insert formatted datetime."
        },
        {
            "type": "datetime",
            "token": "{dd}",
            "format": "HH-mm",
            "description": "Insert formatted datetime."
        },
        {
            "type": "title",
            "token": "{title}",
            "description": "Insert note title from input box.",
            "format": "Untitled"
        },
        {
            "type": "extension",
            "token": "{ext}",
            "description": "Insert file vsnotes.",
            "format": "md"
        }
    ],
    "vsnotes.defaultNoteName": "Note",
}
